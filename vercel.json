{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    }
  ],
  "env": {
    "SUPABASE_URL": "@supabase-url",
    "SUPABASE_SERVICE_KEY": "@supabase-service-key",
    "IMPORT_API_KEY": "@import-api-key"
  },
  "crons": [
    {
      "path": "/api/import",
      "schedule": "0 8 * * 0"
    }
  ]
}
